{
  "name": "skedyul",
  "version": "0.1.15",
  "description": "The Skedyul SDK for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "skedyul": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc --build tsconfig.json && tsc --project tsconfig.tests.json",
    "test": "npm run build && node --test dist-tests/server.test.js"
  },
  "keywords": ["mcp", "skedyul", "serverless", "node", "typescript"],
  "repository": {
    "type": "git",
    "url": "https://github.com/skedyul/skedyul-node"
  },
  "author": "Skedyul <support@skedyul.com>",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "zod": "^4.0.0",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.5.0"
  }
}

